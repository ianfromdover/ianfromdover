{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/react": "^13.3.0",
    "concurrently": "^9.1.2",
    "cra-template": "1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "concurrently \"npm run scss:watch\" \"react-scripts start\" ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "scss:compile": "sass src/styles:build/static/css --style=compressed",
    "scss:watch": "sass src/styles:build/css-live --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.2",
    "sass": "^1.83.4"
  }
}
